<html>
<head>
    <link rel="stylesheet" href="/home/static/{{sourceMap['common.css']}}"/>
    <script src="/home/static/js/security.js"></script>
    <script src="/home/static/{{sourceMap['iconfont']}}" async id="iconfont"></script>
    <script src="/home/static/js/event-emitter.min.js"></script>
    <script src="/home/static/{{sourceMap['tip']}}" async></script>
    <style>
        .iScrollVerticalScrollbar .iScrollIndicator{
            border: 0 !important;
        }
    </style>
    <script>
        window.webWorkerMap = {{webWorkerMap | safe}};
        window.imgMap = {{imgMap | safe}};
        window.emitter = new EventEmitter();
        window.addEventListener('resize', () => {
            emitter.emit('resize');
        });
        window.coinTagOpen = '{{ switch.coin_tag_open }}';
        window.publicInfo = {
            skin: {{ skin | dump | safe}},
            lan: {
                defLan: "{{ lan.defLan }}"
            }
        }
    </script>
    <script inline-html></script>
</head>
<body class="inx b-2-cl c-2-bg Chainer">
{% include "modules/header/china.njk" %}
<div class="page-content page-content-chain" style="position: relative; min-height: 800px" id="app">
    {% include templateModule %}
</div>
{% include "modules/footer/china.njk" %}
<script>
    {{ msg.config_footer_js | safe }}
</script>
</body>
</html>
