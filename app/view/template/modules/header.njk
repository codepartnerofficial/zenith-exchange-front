    <script>
        (function () {
            const $commonHeader = document.querySelector('#common-header');
            const $headerCusNav = document.querySelector('#header-cus-nav');
            const $headerNavEven = document.querySelector('#common-header-linkList');
            const headerNavEvenList = $headerNavEven.querySelectorAll('.header-navEven');
            const {myStorage, getCookie, formatTime, setCookie } = window.BlockChainUtils;
            const $lanList = $commonHeader.querySelectorAll('.lang-list');
            const $loginBtn = $commonHeader.querySelector('.header-login-button');
            const $registerBtn = $commonHeader.querySelector('.header-reg-button');
            const $colorList = document.querySelectorAll('#colorList li');
            const $colorSet = document.querySelector('#color-set');
            let colorDialog = null;

            const bindEvent = () => {
                if ($colorSet){
                    $colorSet.addEventListener('click', function () {
                        colorDialog.show();
                    }, false);
                }

                $commonHeader.querySelectorAll('.goMessage').forEach((item) => {
                    item.addEventListener('click', () => {
                        location.href = '/mesage';
                    }, false);
                });

                $commonHeader.querySelectorAll('.goPersonal').forEach((item) => {
                    item.addEventListener('click', () => {
                        location.href = '/personal/userManagement';
                    }, false);
                });
                $loginBtn.addEventListener('click', (e) => {
                    location.href = '/login';
                });
                $registerBtn.addEventListener('click', (e) => {
                    location.href = '/register';
                });

                $registerBtn.addEventListener('mouseover', (e) => {
                    $registerBtn.className = 'common-button header-reg-button common-button-hollow-big u-8-cl u-8-bd';
                });
                $registerBtn.addEventListener('mouseout', (e) => {
                    $registerBtn.className = 'common-button header-reg-button common-button-hollow-big u-11-cl u-8-bd ';
                });
                $loginBtn.addEventListener('mouseover', (e) => {
                    $loginBtn.className = 'common-button header-login-button common-button-text-kind u-11-cl u-12-bg';
                });
                $loginBtn.addEventListener('mouseout', (e) => {
                    $loginBtn.className = 'common-button header-login-button common-button-text-kind u-11-cl';
                });
                $lanList.forEach((item) => {
                    item.addEventListener('mouseover', (e) => {
                        const target = e.target;
                        if (target.tagName === 'LI') {
                            target.className = 'h-4-bg h-7-cl';
                        }
                    });

                    item.addEventListener('mouseout', (e) => {
                        const target = e.target;
                        if (target.tagName === 'LI') {
                            target.className = 'b-2-cl';
                        }
                    });

                    item.addEventListener('click', (e) => {
                        const target = e.target;
                        const data = target.dataset;
                        if (target.tagName === 'LI') {
                            location.href = data.link;
                        }
                    });
                })


                $headerNavEven.addEventListener('click', (e) => {
                    const target = e.target;
                    const data = target.dataset;
                    const link = data.link;
                    const tar = data.target;
                    const trades = data.trades;
                    const id = data.id;
                    if (trades) {
                        if (id === 'exTrade' && this.etfOpen) {
                            myStorage.set('markTitle', '');
                            myStorage.set('sSymbolName', '');
                        }
                    }
                    if (tar === 'black') {
                        window.open(link);
                    } else {
                        window.location.href = link;
                    }
                }, false);
                $headerNavEven.addEventListener('mouseout', () => {
                    headerNavEvenList.forEach((item) => {
                        item.className = 'header-navEven';
                    });
                }, false);
                $headerNavEven.addEventListener('mouseover', (e) => {
                    const target = e.target;
                    headerNavEvenList.forEach((item) => {
                        item.className = 'header-navEven';
                    });
                    if (target.className === 'header-navEven') {
                        target.className = 'header-navEven h-7-cl';
                    }
                }, false);

                $headerCusNav.addEventListener('mouseover', (e) => {
                    const target = e.target;
                    if (target.tagName === 'A') {
                        target.className = 'h-4-bg h-7-cl';
                    }
                }, false);
                $headerCusNav.addEventListener('mouseout', (e) => {
                    const target = e.target;
                    if (target.tagName === 'A') {
                        target.className = 'x-2-cl g-3-cl-h';
                    }
                }, false);
            };

            const login = () => {
                fetch('/fe-ex-api/common/user_info', {
                    method: 'post',
                    headers: {
                        "exchange-token": getCookie('token'),
                        "exchange-language": getCookie('lan'),
                    },
                }).then((res) => {
                    return res.json();
                }).catch((error) => {
                })
                .then((data) => {
                    if (data) {
                        if (!Number(data.code)) {
                            const $loginSet = $commonHeader.querySelectorAll('.login-set');
                            $loginSet.forEach((target) => {
                                target.style.display = 'block';
                            });
                            const $unloginSet = $commonHeader.querySelectorAll('.unlogin-set');
                            $unloginSet.forEach((target) => {
                                target.style.display = 'none';
                            });
                            $commonHeader.querySelector('.userText').innerHTML = data.data.userAccount;
                            const userState = {
                                0: "{{locale.header.userStatus1}}",
                                1: "{{locale.header.userStatus2}}",
                                2: "{{locale.header.userStatus3}}"
                            };
                            $commonHeader.querySelector('.userStatus').innerHTML = `{{locale.header.userStatus}}: ${userState[Number(data.data.accountStatus)]}`;
                        }
                    }
                });
            };

            const getMessage = () => {
                fetch('/fe-ex-api/message/v4/get_no_read_message_count', {
                    method: 'post',
                    headers: {
                        "exchange-token": getCookie('token'),
                        "exchange-language": getCookie('lan'),
                        "content-type": 'application/json',
                    },
                    body: JSON.stringify({
                        uaTime: formatTime(new Date().getTime())
                    })
                }).then((res) => {
                    return res.json();
                }).catch((error) => {
                })
                    .then((data) => {
                        if (!Number(data.code)) {
                           if (data.data.noReadMsgCount){
                               const $headerUserMessage = $commonHeader.querySelector('.header-user-message');
                               $commonHeader.querySelector('#messageMore').style.display = 'block';
                               $headerUserMessage.classList.add('message-list');
                               const userMessageList = data.data.userMessageList;
                               let _html = ''
                               userMessageList.forEach((item) => {
                                   _html += `<li class="x-2-cl mesageNav">
                                    ${item.messageContent}
                                </li>`;
                               });
                               $headerUserMessage.querySelector('.header-user-text').innerHTML = _html;
                           }
                        }
                    });
            };

            const getData = () => {
                login();
                getMessage();
            };

            const initColorDiaolog = () => {
                $colorList.forEach((item) => {
                    if(item.dataset.checked){
                        item.classList.add('checked');
                    }
                });
                colorDialog = new BlockChainDialog({
                    locale: {
                        titleText: '{{locale.header.set}}',
                        closeTextProps: '{{locale.components.dialog.closeText}}',
                        confirmTextProps: '{{locale.components.dialog.confirmText}}'
                    },
                    content: `<div class="setBox">
                            <div class="setColor clearfix">
                                <div class="setColor-key x-2-cl">{{locale.header.color}}</div>
                                <ul class="setColor-value colorList">
                                   ${ document.querySelector('#colorList').innerHTML}
                                </ul>
                            </div>
                        </div>`,
                    confirm: function (){
                        const $checked = this.dialog.querySelector('.checked');
                        setCookie('cusSkin', $checked.dataset.id);
                        window.location.reload();
                    }
                });

                const $DialogColorList = colorDialog.dialog.querySelectorAll('.colorList li');
                $DialogColorList.forEach((item, inx) => {
                    item.addEventListener('click', (e) => {
                        if (!item.classList.contains('checked')){
                            $DialogColorList.forEach((el) => {
                                el.classList.remove('checked');
                            });
                            item.classList.add('checked');
                        }
                    });
                });
            };
            getData();
            initColorDiaolog();
            bindEvent();


        })();
    </script>
</header>
