          <use xlink:href="#icon-v5_2_1"></use>
        </svg>
      </div>
      <div class="header-user-icon icon-active goPersonal">
        <svg class="icon icon-18" aria-hidden="true">
          <use xlink:href="#icon-v5_2_h"></use>
        </svg>
      </div>
      <div class="header-user-market bg-white">
        <ul class="header-user-text">
          <li class="v5-1-cl userText goPersonal"></li>
          <li class="list-item-cl userStatus"></li>
        </ul>
        <div class="header-user-out hover-bg logout">
          <span class="v5-1-cl">
            <!-- 退出 -->
            {{ locale.header.out }}
          </span>
        </div>
      </div>
    </div>
    <div class="header-lang">
      <div class="header-isLogin-navEven-title v5-5-cl">
        <span class="link-btn">
          {% for item in lanList %} {% if currenLan === item.id %}
          {{item.name}}
          {% endif %} {% endfor %}
        </span>
      </div>
      <ul class="lang-list bg-white list-item-cl">
        {% for item in lanList %} {% if currenLan === item.id %}
        <li data-key="/{{ item.id }}" class="v5-1-cl hover-bg">
          {{ item.name }}
        </li>
        {% else %}
        <li data-key="/{{ item.id }}" class="list-item-cl">
          {{ item.name }}
        </li>
        {% endif %} {% endfor %}
      </ul>
