        opacity: 1;
      }
    }

    @-webkit-keyframes drop-in {
      0% {
        top: 35%;
        opacity: 0.7;
      }

      100% {
        /* margin-top: 10px; */
        opacity: 1;
      }
    }

    @-o-keyframes drop-in {
      0% {
        top: 35%;
        opacity: 0.7;
      }

      100% {
        /* margin-top: 10px; */
        opacity: 1;
      }
    }

    @keyframes drop-in {
      0% {
        top: 35%;
        opacity: 0.7;
      }

      100% {
        /* margin-top: 10px; */
        opacity: 1;
      }
