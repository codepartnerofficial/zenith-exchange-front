  <!-- 主流板块 -->
  <ul class="common-header-linkList" id="common-header-linkList">
    <!-- logo -->
    <li class="linkList-logo-li">
      <a class="linkList-logo" href="{{ headerLink.home }}">
        <img src="{{ msg.logoUrl }}" />
      </a>
    </li>
    {% for item in headerNavList %}
    <li
      class="header-navEven v5-5-cl"
      data-link="{{ item.httpUrl }}"
      data-id="{{ item.id }}"
      data-trades="true"
      data-target="false"
    >
      {{ item.title }}
      {% if item.childList.length > 0 %}
      <span class="icon-triangle">
        <svg class="icon icon-8 icon-static" aria-hidden="true">
          <use xlink:href="#icon-v5_1_1"></use>
        </svg>
        <svg class="icon icon-8 icon-scroll" aria-hidden="true">
          <use xlink:href="#icon-v5_1"></use>
        </svg>
        <svg class="icon icon-8 icon-hover" aria-hidden="true">
          <use xlink:href="#icon-v5_1_h"></use>
        </svg>
      </span>
      <ul class="header-navEven-list bg-white">
        {% for i in item.childList%}
        <li
          class="list-item-cl"
          data-link="{{ i.link }}"
          data-id="{{ i.id }}"
          data-trades="true"
          data-target="false"
        >
          <img src="{{i.logo}}" alt="">
          <dl>
            <dt class="list-item-cl">{{i.title}}</dt>
            <dd class="list-item-cl-sub">{{i.subTitle}}</dd>
          </dl>
        </li>
        {% endfor %}
      </ul>
      {% endif %}
    </li>
    {% endfor %}
  </ul>
  <!-- 公共版款 -->
  <div class="common-header-optionList">
    <!-- 未登录时 -->
    <div class="header-guide unlogin-set">
      <button
        class="common-button header-login-button common-button-text-kind"
        style="margin-top: 0px;vertical-align: middle;"
      >
        <div class="common-button-slot link-btn">
          {{ locale.header.login }}
        </div>
      </button>
      <button
        class="common-button header-reg-button common-button-hollow-big v5-1-bg"
        style="height: 30px; padding-left: 20px; padding-right: 20px; margin-top: 0px; color: #ffffff";
      >
        <div class="common-button-slot">
          {{ locale.header.register }}
        </div>
        <div class="btn-hover"></div>
      </button>
    </div>
    <div class="header-isLogin login-set" style="display: none">
      <!-- 资产 -->
      <div class="header-isLogin-navEven">
        <div class="header-isLogin-navEven-title v5-5-cl">
          <span class="link-btn">{{ locale.header.assets }}</span>
        </div>
        <ul class="lang-list bg-white">
          {% for item in assetsList %}
          <li class="list-item-cl" data-link="{{ item.link }}">{{ item.title }}</li>
          {% endfor %}
        </ul>
      </div>
      <!-- 订单 -->
      <div class="header-isLogin-navEven">
        <div class="header-isLogin-navEven-title v5-5-cl">
          <span class="link-btn">
            {{ locale.header.order }}
          </span>
        </div>
        <ul class="lang-list bg-white">
          <!--  || activeMark === item.link -->
          {% for item in orderList %}
          <li class="list-item-cl" data-link="{{ item.link }}">{{ item.title }}</li>
          {% endfor %}
        </ul>
      </div>
    </div>
    <!-- app下载 -->
    {% if appDownLoad.app_page_url %}
    <div class="header-user appdownLoad-coin">
      <div class="header-user-apphove">
        <div class="header-user-icon icon-info v5-5-cl link-btn">
          {{locale.h5Add.download}}
        </div>
        <div class="header-user-icon icon-active link-btn v5-1-cl">
          {{locale.h5Add.download}}
        </div>
      </div>
      <div class="header-user-appdown bg-white">
        <div class="code-box">
          <div class="download-options-code">
            <img src="{{ appDownLoad.app_page_url }}" alt="" />
          </div>
          <!-- 扫描二维码下载 -->
          <p class="download-options-codeText list-item-cl">
            {{ locale.appDownLoad.headerCode }}
          </p>
        </div>

        <div class="download-options-buttons">
          <button
            class="common-button common-button-solid v5-1-bg download-button"
          >
            <div class="common-button-slot">
              {{ locale.appDownLoad.more }}
            </div>
            <div class="btn-hover"></div>
          </button>
        </div>
      </div>
    </div>
    {% endif %}
    <!-- 设置 -->
    {% if colorList.length %}
    <div class="header-user" id="color-set">
      <div class="header-user-icon icon-info icon-scroll">
        <svg class="icon icon-18" aria-hidden="true">
          {% if cusSkinId == '1' %}
          <use xlink:href="#icon-v5_9"></use>
          {% else %}
          <use xlink:href="#icon-v5_8"></use>
          {% endif %}
        </svg>
      </div>
      <div class="header-user-icon icon-info icon-static">
        <svg class="icon icon-18" aria-hidden="true">
          {% if cusSkinId == '1' %}
          <use xlink:href="#icon-v5_9_1"></use>
          {% else %}
          <use xlink:href="#icon-v5_8_1"></use>
          {% endif %}
        </svg>
      </div>
      <div class="header-user-icon icon-active">
        <svg class="icon icon-18" aria-hidden="true">
          {% if cusSkinId == '1' %}
          <use xlink:href="#icon-v5_9_h"></use>
          {% else %}
          <use xlink:href="#icon-v5_8_h"></use>
          {% endif %}
        </svg>
      </div>
    </div>
    {% endif %}
    <!-- 消息中心 -->
    <div class="header-user login-set" style="display: none">
      <div class="messageCount" id="messageMore" style="display: none"></div>
      <div class="header-user-icon icon-info goMessage icon-scroll">
        <svg class="icon icon-18" aria-hidden="true">
          <use xlink:href="#icon-v5_5"></use>
        </svg>
      </div>
      <div class="header-user-icon icon-info goMessage icon-static">
        <svg class="icon icon-18" aria-hidden="true">
          <use xlink:href="#icon-v5_5_1"></use>
        </svg>
      </div>
      <div class="header-user-icon icon-active goMessage">
        <svg class="icon icon-18" aria-hidden="true">
          <use xlink:href="#icon-v5_5_h"></use>
        </svg>
      </div>
      <div class="header-user-message bg-white goMessage">
        <ul class="header-user-text"></ul>
        <div class="header-user-out hover-bg v5-1-cl">
          {{ locale.header.more }}
        </div>
      </div>
    </div>
    <!-- 个人中心 -->
    <div class="header-user login-set" style="display: none">
      <div class="header-user-icon icon-info goPersonal icon-scroll">
        <svg class="icon icon-18" aria-hidden="true">
          <use xlink:href="#icon-v5_2"></use>
        </svg>
