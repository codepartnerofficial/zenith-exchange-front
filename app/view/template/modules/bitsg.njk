<section class="internationalHome bitsg-home-container">
    <style>
        .bitsg-custom-about {
            height: 550px;
            background: url("https://saas-oss.oss-cn-hongkong.aliyuncs.com/upload/20200308201426985.png") no-repeat center center;
        }
        .custom-about-content {
            width: 1200px;
            margin: 0 auto;
        }
        .custom-about-left {
            float: left;
        }
        .custom-about-leftT {
            margin-top: 120px;
        }
        .top-line {
            font-family: Helvetica;
            font-size: 14px;
            color: #979797;
            letter-spacing: 0;
            line-height: 24px;
        }
        .top-title {
            display: inline-block;
            width: 25px;
            height: 7px;
            border-radius: 2px;
        }
        .main-title {
            font-family: Helvetica-Bold;
            font-size: 58px;
            color: #282830;
            letter-spacing: 0;
            line-height: 70px;
        }
        .main-content {
            width: 400px;
            font-family: Helvetica;
            font-size: 18px;
            color: #979797;
            letter-spacing: 0;
            line-height: 28px;
            margin-top: 40px;
        }
        .custom-about-right {
            float: right;
            position: relative;
        }
        .custom-about-item1 {
            background: #fff;
            box-shadow: 0 2px 47px 6px rgba(195,195,195,0.5);
            border-radius: 4px;
            height: 190px;
            width: 280px;
            padding: 30px 27px;
            box-sizing: border-box;
            position: absolute;
            overflow: hidden;
        }
        .custom-about-item2 {
            background: #fff;
            box-shadow: 0 2px 47px 6px rgba(195,195,195,0.5);
            border-radius: 4px;
            height: 175px;
            width: 305px;
            padding: 35px 20px;
            box-sizing: border-box;
            position: absolute;
            overflow: hidden;
        }
        .advantage1-icon {
            background: url("https://saas-oss.oss-cn-hongkong.aliyuncs.com/upload/20200308205326919.png") no-repeat left center;
            width: 30px;
            height: 33px;
        }
        .advantage2-icon {
            background: url("https://saas-oss.oss-cn-hongkong.aliyuncs.com/upload/20200308210745775.png") no-repeat left center;
            width: 30px;
            height: 30px;
        }
        .advantage3-icon {
            background: url("https://saas-oss.oss-cn-hongkong.aliyuncs.com/upload/20200308210800420.png") no-repeat left center;
            width: 27px;
            height: 30px;
        }
        .advantage4-icon {
            background: url("https://saas-oss.oss-cn-hongkong.aliyuncs.com/upload/20200308210816614.png") no-repeat left center;
            width: 38px;
            height: 33px;
        }
        .advantage-title {
            font-family: Helvetica-Bold;
            font-size: 24px;
            color: #373850;
            line-height: 30px;
            margin: 0 7px;
        }
        .advantage-content {
            font-family: Helvetica;
            font-size: 14px;
            color: #b1b1b1;
            letter-spacing: 0;
            line-height: 20px;
        }
        .advantage1 {
            top: -8px;
            left: -610px;
        }
        .advantage2 {
            top: -80px;
            left: -305px;
        }
        .advantage3 {
            top: 230px;
            left: -630px;
        }
        .advantage4 {
            top: 190px;
            left: -305px;
        }
        .bitsg-custom-data {
            height: 250px;
            background: url("https://saas-oss.oss-cn-hongkong.aliyuncs.com/upload/20200308213743496.png") no-repeat center center;
        }
        .custom-data-content {
            width: 1200px;
            margin: 0 auto;
            padding: 90px 150px 0;
            box-sizing: border-box;
            height: 100%;
        }
        .custom-data-item {
            float: left;
            width: 300px;
            text-align: center;
        }
        .data-item-number {
            font-family: Helvetica-Bold;
            font-size: 38px;
            color: #f0b90a;
            letter-spacing: 0;
        }
        .data-item-line {
            height: 1px;
            width: 38px;
            background: #f0b90a;
            margin: 5px auto;
        }
        .data-item-title {
            font-family: Helvetica;
            font-size: 14px;
            color: #9e9e9e;
            letter-spacing: 0;
            text-align: center;
            line-height: 24px;
        }
        .bitsg-custom-download {
            background: #fff;
        }
        .bitsg-custom-download .custom-download-content {
            width: 1200px;
            height: auto;
            margin: 0 auto;
        }
        .download-content-top {
            height: 300px;
        }
        .download-content-topL {
            float: left;
            margin-top: 120px;
        }
        .download-content-topR {
            float: right;
            margin-top: 160px;
        }
        .downloadButton {
            display: inline-block;
            width: 150px;
            height: 40px;
            border: 2px solid #f0b90a;
            text-align: center;
            line-height: 40px;
            border-radius: 4px;
            color: #f0b90a;
        }
        .content-download {
            height: 1000px;
        }
        .content-left-img {
            float: left;
            width: 700px;
        }
        .left-img {
            width: 100%;
            height: 100%;
        }
        .content-left-img .left-img{
            height: auto;
        }
        .margin-img {
            margin-left: -100px;
        }
        .content-right {
            float: right;
            width: 500px;
            margin-top: 30px;
        }
        .content-right-text {
            margin-bottom: 70px;
            overflow: hidden;
        }
        .right-text-icon {
            float: left;
            width: 44px;
            height: 42px;
            margin-right: 20px;
        }
        .right-text-con {
            float: left;
            width: 436px;
        }
        .right-text1 {
            font-family: PingFangSC-Semibold;
            font-size: 24px;
            color: #373850;
            text-align: left;
            margin-bottom: 20px;
        }
        .right-text2 {
            font-family: PingFangSC-Regular;
            font-size: 18px;
            color: #6e738e;
            text-align: left;
            line-height: 28px;
        }
        .app-download {
            margin-top: 20px;
            margin-left: 64px;
        }
        .down-btn {
            width: 160px;
            height: 40px;
            float: left;
            cursor: pointer;
            margin: 6px 8px 0 0;
            position: relative;
            display: inline-block;
        }
        .down-btn-2 {
            margin-right: 0;
        }
        .bitsg-ewm {
            float: left;
            width: 64px;
            height: 64px;
            position: relative;
            cursor: pointer;
            margin-top: -4px;
        }
        .bug-ewm {
            position: absolute;
            background: #fff;
            border: 0 solid #ddd;
            bottom: 70px;
            right: 11px;
            width: 120px;
            height: 150px;
            padding: 15px;
            box-shadow: 0 2px 10px 0 rgba(0,0,0,0.1);
            border-radius: 4px;
            display: none;
        }
        .ewm-img {
            border: 1px solid #d5dee7;
            border-radius: 4px;
            padding: 1px;
            display: block;
            width: 120px;
            height: 120px;
            box-sizing: border-box;
        }
        .bitsg-ewm:hover > .bug-ewm {
            display: block;
        }
        .ewm-text {
            font-size: 14px;
            font-weight: 400;
            color: #35384a;
            line-height: 20px;
            margin-top: 10px;
            text-align: center;
        }
        .bitsg-custom-logo {
            height: 900px;
            background: #f9fafc;
        }
        .custom-logo-mtop {
            width: 1200px;
            margin: 0 auto;
        }
        .custom-logo-top {
            box-sizing: border-box;
            height: 300px;
            padding-top: 120px;
        }
        .logo-list {
            overflow: hidden;
            height: 600px;
            width: 100%;
            position: relative;
        }
        .logo-list-all {
            width: 4400px;
            animation: scrollLogo1 60s infinite linear;
            position: absolute;
            top: 0;
            left: 0;
        }
        .log-list-scroll {
            width: 2200px;
            float: left;
        }
        .logo-item {
            height: 170px;
            width: 360px;
            float: left;
        }
        .log-list-scroll > img {
            height: 100%;
        }
        .marginR {
            margin-right: 30px;
        }
        .marginL {
            margin-left: 135px;
        }
        .nomargin {
            margin-right: -45px;
        }
        .marginL1 {
            margin-left: 270px;
        }
        .marginL2 {
            margin-left: 540px;
        }
        .logo-line1 {
            overflow: hidden;
        }
        .logo-line2 {
            overflow: hidden;
            margin-top: -58px;
        }
        .logo-line3 {
            overflow: hidden;
            margin-top: -46px;
        }
        .logo-line4 {
            overflow: hidden;
            margin-top: -40px;
        }
        .bitsg-custom-join {
            height: 350px;
            background: url("https://saas-oss.oss-cn-hongkong.aliyuncs.com/upload/20200309005748269.png") no-repeat center center;
        }
        .custom-join-content {
            width: 1200px;
            margin: 0 auto;
            height: 100%;
            text-align: center;
            padding-top: 100px;
        }
        .custom-join-p1 {
            font-family: Helvetica-Bold;
            font-size: 40px;
            color: #fff;
        }
        .custom-join-p2 {
            font-family: Helvetica;
            font-size: 14px;
            color: #fff;
            line-height: 16px;
            margin-top: 22px;
        }
        .custom-join-p3 {
            width: 240px;
            margin: 28px auto 0;
        }
        .custom-join-p3 > a {
            display: block;
            height: 50px;
            width: 240px;
            border-radius: 4px;
            background: #0f0d1a;
            text-align: center;
            line-height: 50px;
            color: #fff;
        }
        @-moz-keyframes scrollLogo1 {
            0% {
                transform: translateX(0px);
            }
            100% {
                transform: translateX(-2200px);
            }
        }
        @-webkit-keyframes scrollLogo1 {
            0% {
                transform: translateX(0px);
            }
            100% {
                transform: translateX(-2200px);
            }
        }
        @-o-keyframes scrollLogo1 {
            0% {
                transform: translateX(0px);
            }
            100% {
                transform: translateX(-2200px);
            }
        }
        @keyframes scrollLogo1 {
            0% {
                transform: translateX(0px);
            }
            100% {
                transform: translateX(-2200px);
            }
        }

    </style>
    {% import "./swiper/bitsg.njk" as swiper %}
    {{ swiper.boxStyle(
    slideList=helper.swiper.getInterMap(cmsAdvertList),
    extendPath =sourceMap.interSwiperExtend,
    title=internationalTitle.title,
    subTitle=internationalTitle.subTitle,
    goRegister=locale.login.goRegister
    )}}
    {% include "./noticeInfo/bitsg.njk" %}
    {% include "./market/bitsg.njk" %}
    {{ footer_warm_prompt | safe }}
