  <script>
    (() => {
      class customExtend {
        constructor() {
          this.init();
        }

        init() {
          const $custom = document.querySelector('.v5-custom-info');
          const $guide = document.querySelector('.v5-guide');
          const $appDownload = document.querySelector('.v5-appDownload');
          const $service = document.querySelector('.v5-service');
          const $registerGuide = document.querySelector('.v5-register-guide');
          const { clientHeight } =  document.documentElement;
          window.addEventListener('scroll', () => {
            const { scrollY } = window;
            if ($guide) {
              const g_offset = $guide.offsetTop;
              if (g_offset < (clientHeight + scrollY - 100)) {
                $guide.classList.remove('effect-fade');
              }
            }
            if ($appDownload) {
              const a_offset = $appDownload.offsetTop;
              if (a_offset < (clientHeight + scrollY - 100)) {
                $appDownload.classList.remove('effect-fade');
              }
            }
            if ($service) {
              const s_offset = $service.offsetTop;
              if (s_offset < (clientHeight + scrollY - 100)) {
                $service.classList.remove('effect-fade');
              }
            }
            if ($registerGuide) {
              const r_offset = $registerGuide.offsetTop;
              if (r_offset < (clientHeight + scrollY - 100)) {
                $registerGuide.classList.remove('effect-fade');
              }
            }
          })
        }
      }
      new customExtend();
    })()
  </script>
