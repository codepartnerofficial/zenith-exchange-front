<section class="v5-custom-info">
  <style>
    .v5-custom-info {
      width:100%;
      margin: auto;
      font-family: DINPro-Medium;
      font-weight: 500;
      margin-top:120px;
    }
    .v5-custom-info .v5-guide, .v5-custom-info .v5-appDownload, .v5-custom-info .v5-service, .v5-custom-info .v5-register-guide {
      transition: all 0.5s;
    }
    .v5-guide, .v5-appDownload,.v5-service{
      width:1200px;
      margin:0 auto;
    }
    .v5-custom-info .title-text {
      font-size: 36px;
      line-height: 46px;
      font-family: DINPro-Bold;
      font-weight: 700;
    }
    .v5-custom-info .title-tip {
      font-size: 14px;
      line-height: 18px;
      margin-top: 16px;
      font-family: DINPro-Medium;
    }
    .v5-custom-info .v5-guide .guide-content {
      margin-top: 60px;
      display: flex;
      justify-content: space-between;
    }
    .v5-custom-info .v5-guide .guide-item {
      width: 386px;
    }
    .v5-custom-info .v5-guide .guide-item dt {
      width: 100%;
      margin-bottom: 16px;
      position:relative;
    }
    .v5-custom-info .v5-guide .guide-item dt img {
      width: 386px;
      height: 240px;
    }
    .v5-custom-info .v5-guide .guide-item dt span{
      position: absolute;
      top:0;
      left:0;
      width:100%;
      height:100%;
      background:rgba(0,0,0,.1);
      display:none;
    }
    .v5-custom-info .v5-guide .guide-item dt:hover span{
      display:block;
    }
    .v5-custom-info .v5-guide .guide-item dd {
      font-size: 16px;
      line-height: 26px;
    }
    .v5-custom-info .v5-service {
      margin-bottom:160px;
    }
    .v5-custom-info .v5-appDownload  {
      
      padding: 60px 0px 0;
      box-sizing: border-box;
      justify-content: space-between;
    }
    .v5-custom-info .v5-appDownload >div{
      display:flex;
      padding:0 80px
    }
    .v5-custom-info .v5-appDownload >div >div{
      width:47%;
    }
    .v5-custom-info .v5-appDownload >div .app-introduce{
      width:53%
    }
    .v5-custom-info .v5-appDownload  .title-text {
      font-size: 36px;
      line-height: 44px;
      margin-bottom: 14px;
      width:100%;
    }
    .v5-custom-info .v5-appDownload  .title-tip {
      font-size: 16px;
      line-height: 21px;
      margin-bottom: 49px;
      width:100%;
    }
    .v5-custom-info .v5-appDownload .app-preview .intro-item {
      margin-bottom: 28px;
      display: flex;
      overflow:hidden;
    }
   
    .v5-custom-info .v5-appDownload .app-preview .intro-item p {
      font-weight: 700;
      font-size: 16px;
      line-height: 21px;
      font-family: DINPro-Bold;
      margin-bottom: 8px;
    }
    .v5-custom-info .v5-appDownload .app-preview .intro-item span {
      display: inline-block;
      width: 340px;
      font-size: 14px;
      line-height: 18px;
    }
    .v5-custom-info .download {
      height: 44px;
      padding-top: 28px;
      display: flex;
      align-items: center;
      position: relative;
      width:50%
    }
    .v5-custom-info .download img {
      margin-right: 8px;
    }
    .v5-custom-info .download .qrcode .img {
      display: inline-block;
      width: 44px;
      height: 44px;
      box-shadow: 0px 2px 10px 0px rgba(0,0,0,0.1);
      border-radius: 4px;
      padding: 10px;
      box-sizing: border-box;
    }
     .v5-custom-info .app-preview .intro-item svg {
      margin-right: 24px;
      width:24px;
      height:17px;margin-top:10px;
    }
    
    .v5-custom-info .download .download-store {
      width: 160px;
    }
    .v5-custom-info .download  img.left {
      width: 356px;
      height:714px;
    }
    
    .v5-custom-info .download .qrcode-preview {
      width: 0;
      height: 0;
      opacity: 0;
      overflow: hidden;
      text-align: center;
      transition: all 0.3s;
      border-radius: 8px;
      box-shadow: 0px 2px 10px 0px rgba(0,0,0,0.1);
      position: absolute;
      left: 335px;
      bottom: 56px;
      box-sizing: border-box;
      background: #ffffff;
    }
    .v5-custom-info .v5-appDownload .qrcode-preview:hover {
      width: 150px;
      height: 190px;
      opacity: 1;
    }
    .v5-custom-info .download .qrcode-preview img {
      width: 100%;
    }
    .v5-custom-info .download .qrcode-preview span {
      font-size: 14px;
      line-height: 20px;
    }
    
        .v5-custom-info .v5-register-guide {
      width: 100%;
      height: 300px;
      text-align: center;
      padding-top: 66px;
      box-sizing: border-box;
      margin-top: 204px;
    }
    .v5-custom-info .v5-register-guide .title-text {
      margin-bottom: 16px;
    }
    .v5-custom-info .v5-register-guide p {
      font-size: 16px;
      line-height: 21px;
      margin-bottom: 40px;
    }
    .v5-custom-info .v5-register-guide a {
      display: flex;
      margin: auto;
      width: 240px;
      height: 50px;
      box-shadow: 0px 4px 12px 0px rgba(0,0,0,0.1);
      border-radius: 4px;
      padding: 0 20px;
      justify-content: space-between;
      box-sizing: border-box;
      align-items: center;
      color: #ffffff;
      position: relative;
      text-decoration: none;
    }
    .v5-custom-info .v5-register-guide a .btn-hover {
      position: absolute;
      width: 100%;
      height: 100%;
      left: 0;
      top: 0;
      background:rgba(0, 0, 0, 0.1);
      cursor: pointer;
      display: none;
    }
    .v5-custom-info .v5-register-guide a:hover .btn-hover {
      display: block;
    }
    .v5-custom-info .v5-register-guide a span {
      font-size: 16px;
    }
    .v5-service .title-text {
      font-size: 32px;
      line-height: 40px;
      margin-bottom: 90px !important;
    }
    .v5-custom-info .v5-service .service-content {
      display: flex;
      justify-content: space-between;
    }
     .v5-custom-info .v5-service .service-content .service-item {
      width: 230px;
    }
    .v5-custom-info .v5-service .service-content .service-item dt {
      height: 72px;
      line-height: 72px;
      margin-bottom: 50px;
    }
    .v5-custom-info .v5-service .service-content .service-item dd div {
      font-size: 16px;
      line-height: 21px;
      margin-bottom: 13px;
      font-weight: 500;
    }
    .v5-custom-info .v5-service .service-content .service-item dd span {
      font-size: 14px;
      line-height: 24px;
    }
    .v5-custom-info .v5-appDownload {
      margin-top: 100px;
    }
    .v5-custom-info .download .qrcode .hoverShow {
      display: none;
    }
    .v5-custom-info .download .qrcode .hoverHide {
      background: #ffffff;
    }
    .v5-custom-info .download .qrcode:hover .hoverShow {
      display: inline-block;
    }
    .v5-custom-info .download .qrcode:hover .hoverHide {
      display: none;
    }
    .v5-custom-info  .download .qrcode:hover .qrcode-preview {
      width: 150px;
      height: 190px;
      opacity: 1;
    }
    .v5-custom-info .v5-appDownload .app-introduce img{
      width:80%;
    }
    .v5-custom-info .app-preview ul{
      margin-bottom:50px
    }
    .v5-custom-info .app-preview{
      margin-top:100px;
    }
  </style>
