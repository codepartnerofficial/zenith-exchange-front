  <script>
    (() => {
      class customExtend {
        constructor() {
          this.init();
        }

        init() {
          this.$custom = document.querySelector('.v5-custom-info');
          this.$guide = document.querySelector('.v5-guide');
          this.$appDownload = document.querySelector('.v5-appDownload');
          this.$service = document.querySelector('.v5-service');
          this.$registerGuide = document.querySelector('.v5-register-guide');
          this.clientHeight =  document.documentElement.clientHeight;
          this.setFade();
          window.addEventListener('scroll', () => {
            this.setFade();
          })
        }

        setFade() {
          const { scrollY } = window;
          if (this.$guide) {
            const g_offset = this.$guide.offsetTop;
            if (g_offset < (this.clientHeight + scrollY - 100)) {
              this.$guide.classList.remove('effect-fade');
            }
          }
          if (this.$appDownload) {
            const a_offset = this.$appDownload.offsetTop;
            if (a_offset < (this.clientHeight + scrollY - 100)) {
              this.$appDownload.classList.remove('effect-fade');
            }
          }
          if (this.$service) {
            const s_offset = this.$service.offsetTop;
            if (s_offset < (this.clientHeight + scrollY - 100)) {
              this.$service.classList.remove('effect-fade');
            }
          }
          if (this.$registerGuide) {
            const r_offset = this.$registerGuide.offsetTop;
            if (r_offset < (this.clientHeight + scrollY - 100)) {
              this.$registerGuide.classList.remove('effect-fade');
            }
          }
        }
      }
      new customExtend();
    })()
  </script>
